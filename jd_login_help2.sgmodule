京东商品 = type=http-response,pattern=^https:\/\/item\.m\.jd\.com\/product\/\s+\.html,requires-body=1,max-size=0,timeout=1000,script-path=https://raw.githubusercontent.com/dompling/Script/master/jd/jd_login_help2.js
京东店铺 = type=http-response,pattern=^https:\/\/shop\.m\.jd\.com\/shopv2\/mzpage?venderId,requires-body=1,max-size=0,timeout=1000,script-path=https://raw.githubusercontent.com/dompling/Script/master/jd/jd_login_help2.js
京东入会 = type=http-response,pattern=^https:\/\/shopmember\.m\.jd\.com,requires-body=1,max-size=0,timeout=1000,script-path=https://raw.githubusercontent.com/dompling/Script/master/jd/jd_login_help2.js
京东销卡 = type=http-response,pattern=^https:\/\/cardpackage\.jd\.com,requires-body=1,max-size=0,timeout=1000,script-path=https://raw.githubusercontent.com/dompling/Script/master/jd/jd_login_help2.js
