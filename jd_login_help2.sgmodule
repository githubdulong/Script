京喜农场 = type=http-response,pattern=^https:\/\/actst\.jingxi\.com\/sns\/201912\/12\/jxnc\/detail\.html,requires-body=1,max-size=0,timeout=1000,script-path=https://raw.githubusercontent.com/dompling/Script/master/jd/jd_login_help2.js
京喜财富 = type=http-response,pattern=^https:\/\/st\.jingxi\.com\/fortune_island\/index\.html,requires-body=1,max-size=0,timeout=1000,script-path=https://raw.githubusercontent.com/dompling/Script/master/jd/jd_login_help2.js
