#!name=京东系辅助
#!desc=京东+京喜账号CK辅助登陆(UI_2)
#!system=ios

[Script]
# > 京东系列
京东首页 = type=http-response,pattern=^https:\/\/m\.jd\.com,requires-body=1,max-size=0,timeout=1000,script-path=https://raw.githubusercontent.com/dompling/Script/master/jd/jd_login_help2.js
京东登陆 = type=http-response,pattern=^https:\/\/plogin\.m\.jd\.com,requires-body=1,max-size=0,timeout=1000,script-path=https://raw.githubusercontent.com/dompling/Script/master/jd/jd_login_help2.js
京东我的 = type=http-response,pattern=^https:\/\/home\.m\.jd\.com\/myJd\/newhome\.action,requires-body=1,max-size=0,timeout=1000,script-path=https://raw.githubusercontent.com/dompling/Script/master/jd/jd_login_help2.js
京东购物 = type=http-response,pattern=^https:\/\/p\.m\.jd\.com\/cart\/cart\.action,requires-body=1,max-size=0,timeout=1000,script-path=https://raw.githubusercontent.com/dompling/Script/master/jd/jd_login_help2.js
